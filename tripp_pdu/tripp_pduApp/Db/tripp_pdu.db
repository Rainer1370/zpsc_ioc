# Tripp PDU Load Control and Status Records for 8 Outlets

record(bo, "$(P):OUT1:CTRL") {
    field(DESC, "Control Outlet 1")
    field(ZNAM, "OFF")
    field(ONAM, "ON")
    field(SCAN, "Passive")
}

record(bi, "$(P):OUT1:STAT") {
    field(DESC, "Status of Outlet 1")
    field(ZNAM, "OFF")
    field(ONAM, "ON")
    field(SCAN, "1 second")
}

