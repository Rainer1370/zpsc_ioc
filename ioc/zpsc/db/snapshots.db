record(waveform,"$(P){$(NO)}Chan$(CHAN):$(TYPE):DCCT1-Wfm") {
    field(DTYP, "PSC Block I32 In")
    field(INP , "@Wfm$(NO) $(MSGID) 8 40")
    field(SCAN,"I/O Intr")
    field(FTVL,"DOUBLE")
    field(NELM,"$(BUFLEN)")
    field(FLNK, "$(P){$(NO)}Chan$(CHAN):$(TYPE):TriggerScaleDCCT1") 
}

record(ai, "$(P){$(NO)}Chan$(CHAN):DCCT1ScaleFactor-I") {
    field(DTYP, "Soft Channel")
    field(VAL, "1.0")   
    field(PINI, "YES")
    field(FLNK, "$(P){$(NO)}Chan$(CHAN):$(TYPE):TriggerScaleDCCT1") 
}

record(fanout, "$(P){$(NO)}Chan$(CHAN):$(TYPE):TriggerScaleDCCT1") {
    field(LNK1, "$(P){$(NO)}Chan$(CHAN):$(TYPE):ScaleDCCT1")
}

record(aSub, "$(P){$(NO)}Chan$(CHAN):$(TYPE):ScaleDCCT1") {
    field(INPA, "$(P){$(NO)}Chan$(CHAN):$(TYPE):DCCT1-Wfm")
    field(FTA , "DOUBLE")
    field(NOA , "$(BUFLEN)")
    field(INPB, "$(P){$(NO)}Chan$(CHAN):DCCT1ScaleFactor-I")
    field(FTB , "DOUBLE")
    field(NOB , "1")
    field(OUTA, "$(P){$(NO)}Chan$(CHAN):$(TYPE):DCCT1Scaled-Wfm PP")
    field(FTVA, "DOUBLE")
    field(NOVA, "$(BUFLEN)")
    field(SNAM, "scaleWaveform")
    field(SCAN, "Passive") 
    field(FLNK, "$(P){$(NO)}Chan$(CHAN):$(TYPE):DCCT1Scaled-Wfm")
}

